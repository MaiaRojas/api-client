{
  "name": "api-client",
  "version": "1.2.0",
  "description": "Laboratoria API client made for redux apps",
  "main": "build/index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --verbose --coverage",
    "build": "babel lib --out-dir build",
    "changelog": "git log $(git describe --tags --abbrev=0)..HEAD --oneline --format=\"%h %s (%an <%ae>)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laboratoria/api-client.git"
  },
  "keywords": [
    "api",
    "client",
    "laboratoria",
    "redux"
  ],
  "author": "Laboratoria <sumate@laboratoria.la> (http://laboratoria.la/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Laboratoria/api-client/issues"
  },
  "homepage": "https://github.com/Laboratoria/api-client#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "jest-mock-axios": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0"
  },
  "eslintIgnore": [
    "build/**/*"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
